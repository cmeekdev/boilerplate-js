{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate javascript project",
  "main": "webpack.config.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "server": "node ./dist/src/server/app.js",
    "test": "mocha ./test-setup.js ./src/**/__tests__/*.test.js --compilers js:babel-core/register",
    "cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/mocha ./test-setup.js ./src/**/__tests__/*.test.js --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmeekdev/boilerplate.git"
  },
  "keywords": [
    "Boilerplate"
  ],
  "author": "Chris Meek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmeekdev/boilerplate/issues"
  },
  "homepage": "https://github.com/cmeekdev/boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "sinon": "^1.17.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "immutable": "^3.8.1",
    "material-ui": "^0.16.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3"
  }
